@using AutoService.WEB.Models
@model List<ServicesSummaryAdminView>

@if (Model.Count > 0)
{
    <div class="row mx-auto justify-content-center">
        @foreach (var order in Model)
        {
            @Html.Partial("SummaryView", order)
        }
    </div>
}
else
{
    <h2 class="display-3 text-center">На сегодня заказов нет.</h2>
}