@using AutoService.WEB.Models
@model List<ServicesSummaryAdminView>

@if (Model.Count > 0)
{
<div class="row mx-auto justify-content-center">
    @foreach (var order in Model)
        {
    @Html.Partial("SummaryView", order)
        }
</div>
}
else
{

<div class="m-3">
    <h3 class="display-3 text-center">На сегодня заказов нет.</h3>
    <div class="mx-auto d-flex m-2">
        <img class="img-fluid mx-auto" src="https://i.imgur.com/8o7oQm0.png" alt="Alternate Text" />
    </div>
</div>

}