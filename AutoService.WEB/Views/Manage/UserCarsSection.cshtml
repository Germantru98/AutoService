@using AutoService.WEB.Models
@model List<CarView>
<div class="tab-pane fade p-2" id="cars">
    <div class="row justify-content-center mt-4">

        @if (Model.Count == 0)
        {
            @Html.Partial("NewCarCardView")
        }
        else
        {
            foreach (var car in Model)
            {
                @Html.Partial("UserCarView", car)
            }
            @Html.Partial("NewCarCardView")
        }
    </div>
</div>